Cell["\<\
The following function determines by means of linear programming whether a \
vector v lies in the lower convex hull (the set of nonnegative vectors \
bounded above by the convex hull) determined by a set of vectors V.\
\>", "Text",
 CellChangeTimes->{{3.834855546236594*^9, 3.834855582662504*^9}, {
  3.834855668473097*^9, 3.834855682250544*^9}, {3.8348557873484373`*^9, 
  3.834856025081976*^9}, {3.834856059096085*^9, 3.8348564174339457`*^9}, {
  3.834856450083089*^9, 3.834856491437067*^9}, {3.845746690966366*^9, 
  3.845746762129098*^9}},ExpressionUUID->"45fe1c4e-8bf1-445a-9efe-\
dcd8eba78024"],

Cell[BoxData[
 RowBox[{
  RowBox[{"InConvHull", "[", 
   RowBox[{"v_", ",", "V_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"Plus", "@@", 
    RowBox[{"(", 
     RowBox[{"LinearProgramming", "[", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{"1", ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", 
           RowBox[{"Length", "[", "V", "]"}]}], "}"}]}], "]"}], ",", 
       RowBox[{"Transpose", "[", "V", "]"}], ",", "v"}], "]"}], ")"}]}], 
   "\[LessEqual]", "1"}]}]], "Input",
 CellChangeTimes->{{3.8333563372984*^9, 3.833356340528775*^9}, {
  3.834856437330114*^9, 3.834856445934486*^9}},
 CellLabel->"In[3]:=",ExpressionUUID->"ef0cbf19-c415-41ae-9c3f-a893defdbeba"],

Cell["\<\
